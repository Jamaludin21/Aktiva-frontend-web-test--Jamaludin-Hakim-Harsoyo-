{
  "version": 2,
  "routes": [
    {
      "src": "/api/.*",
      "dest": "api.js"
    },
    {
      "src": "/(.*\\.(json|js|jsx|ts|tsx|css|png|jpg|jpeg|gif|svg|eot|otf|webp|ttf|woff|woff2|manifest|webmanifest|xml|txt))$",
      "dest": "src/$1"
    },
    {
      "src": "/.*",
      "dest": "src/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
